class Basic {
//    public int numTrees(int n) {
	public static void main(String args[]) {
        n=3;
        int[] pic = new int[2*n];
        pic[0]=1;
        pic[1]=1;
        for(int i=2;i<2*n;i++){
            pic[n] = i + pic[n-1] ;
        }
        return((pic[2*n-1])/((pic[n]) * pic[n-1]));
    }
}